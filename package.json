{
	"name": "gali-new-handler",
	"version": "1.0.0",
	"description": "A discord bot.",
	"type": "commonjs",
	"main": "main.js",
	"scripts": {
		"canary": "npm run start:canary",
		"start": "npm run start:prod",
		"start:prod": "node --trace-warnings src/main.js",
		"start:canary": "node --trace-warnings src/main.js --canary",
		"prettier": "prettier ./ --write"
	},
	"dependencies": {
		"discord.js": "^12.3.1",
		"figlet": "^1.5.0",
		"godembed": "^1.0.4",
		"imgflip": "^2.0.0",
		"imgur": "^0.3.1",
		"jimp": "^0.16.1",
		"node-fetch": "^2.6.1"
	},
	"keywords": [
		"galileo",
		"discord",
		"bot"
	],
	"author": "Ayfri & Antow",
	"license": "ISC",
	"devDependencies": {
		"prettier": "^2.1.2"
	},
	"prettier": {
		"arrowParens": "avoid",
		"bracketSpacing": false,
		"embeddedLanguageFormatting": "auto",
		"endOfLine": "lf",
		"printWidth": 200,
		"semi": true,
		"singleQuote": true,
		"tabWidth": 4,
		"trailingComma": "es5",
		"useTabs": true
	}
}
